<component>
    <dependencySets>
        <dependencySet>
            <outputDirectory>/msa/lib</outputDirectory>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <directory>src/main/resources</directory>
            <outputDirectory>msa/classes</outputDirectory>
            <includes>
                <include>*.properties</include>
                <include>*.xml</include>
            </includes>
            <filtered>true</filtered>
        </fileSet>
        <fileSet>
            <directory>target/dependency-resources</directory>
            <outputDirectory>msa/classes</outputDirectory>
            <includes>
                <include>*.properties</include>
                <include>*.ccf</include>
                <include>*.xml</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>target</directory>
            <outputDirectory>msa/lib</outputDirectory>
            <excludes>
                <exclude>*sources.jar</exclude>
                <exclude>*javadoc.jar</exclude>
                <exclude>msa.jar</exclude>
            </excludes>
            <includes>
                <include>*.jar</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>appContent/logs</directory>
            <outputDirectory>msa/logs</outputDirectory>
            <includes>
                <include>**/*</include>
            </includes>
        </fileSet>
        <fileSet>
            <outputDirectory>msa</outputDirectory>
            <includes>
                <include>README.md</include>
            </includes>
        </fileSet>
    </fileSets>
</component>
